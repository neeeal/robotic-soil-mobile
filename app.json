{
  "expo": {
    "name": "AgroViz",
    "slug": "robotic-soil-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.neeeal.roboticsoilmobile",
      "config":{
        "googleMaps":{
          "apiKey": "AIzaSyDdTFjPoKas7TZnNwGBKe8s3A9eVGYfIaM"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c87c08fc-44c1-448d-95e4-5707734794be"
      }
    }
  }
}
